    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.0.1",
    "bootstrap": "^5.1.3",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.1.0",
