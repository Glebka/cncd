{
  "name": "cncd",
  "version": "0.1.0",
  "description": "cncd",
  "private": true,
  "scripts": {
    "clean": "rm -rf ui/dist/ && rm -rf node_modules/.vite && rm -rf server/target",
    "start:web": "cd ui/ && vite -m development",
    "start:be": "cd server/ && cargo run",
    "build:web": "cd ui/ && tsc -p ./src/tsconfig.json && vite build",
    "build:be": "cd server/ && cargo build",
    "build:web:dev": "cd ui/ && tsc -p ./src/ui/tsconfig.json && vite build -m development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
